{
  "name": "@curvenote/quantecon-book",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "postinstall": "patch-package --patch-dir patches",
    "compile": "tsc --noEmit --skipLibCheck",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:copy": "cp remix.config.dev.js remix.config.js",
    "prod:copy": "cp remix.config.prod.js remix.config.js",
    "clean": "rimraf public/build build api",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:thebe": "copy-thebe-assets ./public",
    "prod:build": "npm run prod:copy && npm run build:thebe && npm run build:css && remix build",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev": "npm run dev:copy && npm run build:thebe && concurrently \"npm run dev:css\" \"remix dev\"",
    "start": "npm run build:css && remix dev"
  },
  "dependencies": {
    "@myst-theme/common": "^0.13.6",
    "@myst-theme/icons": "^0.13.6",
    "@myst-theme/jupyter": "^0.13.6",
    "@myst-theme/providers": "^0.13.6",
    "@myst-theme/search-minisearch": "^0.13.6",
    "@myst-theme/site": "^0.13.6",
    "@myst-theme/styles": "^0.13.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@remix-run/node": "~1.19.3",
    "@remix-run/react": "~1.19.3",
    "@remix-run/vercel": "~1.19.3",
    "lucide-react": "^0.475.0",
    "myst-common": "^1.7.8",
    "myst-config": "^1.7.8",
    "node-fetch": "^2.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@vercel/node": "^2.15.1",
    "concurrently": "^9.1.2",
    "patch-package": "^8.0.0",
    "prettier": "latest",
    "tailwindcss": "^3.4.17"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "npm@8.10.0"
}
